<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-cascade="save-update">
	<class name="unq.tpi.desapp.model.CommentedPoint" table="COMMENTEDPOINTS">

		<id column="ID_COMMENTEDPOINT" name="id" type="java.lang.Integer">
			<generator class="identity" />
		</id>

		<property name="user" column="ID_USER" type="unq.tpi.desapp.model.User" />

		<property name="isNegative" type="boolean" column="IS_NEGATIVE" />

		<property name="comment" type="string" column="COMMENT" />

	</class>
</hibernate-mapping>
