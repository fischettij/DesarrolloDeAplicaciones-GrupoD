<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-cascade="save-update">
	<class name="unq.tpi.desapp.model.Routine" table="ROUTINES">

		<id column="ID_ROUTINE" name="id" type="java.lang.Integer">
			<generator class="identity" />
		</id>

		<property name="startingDate"
			type="org.joda.time.contrib.hibernate.PersistentLocalDate" column="STARTING_DATE" />

		<property name="endDate"
			type="org.joda.time.contrib.hibernate.PersistentLocalDate" column="END_DATE" />

		<!-- FALTA LA LISTA -->

	</class>
</hibernate-mapping>
