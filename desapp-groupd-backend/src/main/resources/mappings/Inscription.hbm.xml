<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-cascade="save-update">
	<class name="unq.tpi.desapp.model.Inscription" table="INSCRIPTIONS">

		<id column="ID_INSCRIPTION" name="id" type="java.lang.Integer">
			<generator class="identity" />
		</id>

		<many-to-one name="route" column="ID_ROUTE" cascade="all" />

		<property name="isNegative" type="boolean" column="IS_NEGATIVE" />

		<property name="inscriptionState" column="INSCRIPTION_STATE"
			type="unq.tpi.desapp.model.inscription.InscriptionState" />

	</class>
</hibernate-mapping>
