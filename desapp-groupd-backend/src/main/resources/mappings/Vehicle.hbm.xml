<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="unq.tpi.desapp.model.Vehicle" table="VEHICLES">

		<id column="ID_VEHICLE" name="id" type="java.lang.Long">
			<generator class="identity" />
		</id>

		<property name="model" type="string" column="MODEL" />

		<property name="maxNumberPassangers" type="java.lang.Integer" column="MAX_NUMBER_PASSANGERS" />

		<property name="registrationNumber" type="string" column="REGISTRATION_NUMBER" />

	</class>
</hibernate-mapping>
