<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-cascade="save-update">
	<class name="unq.tpi.desapp.model.User" table="USERS">

		<id column="ID_USER" name="id" type="java.lang.Long">
			<generator class="native" />
		</id>

		<property name="name" type="string" column="NAME" />

		<many-to-one name="vehicleManager" column="ID_VEHICLE_MANAGER"
			unique="true" class="unq.tpi.desapp.model.manager.VehicleManager"
			not-null="true" />

		<many-to-one name="routeManager" column="ID_ROUTE_CAMANGER"
			unique="true" class="unq.tpi.desapp.model.manager.RouteManager"
			not-null="true" />

		<many-to-one name="inscriptionManager" column="ID_INSCRIPTION_MANAGER"
			unique="true" class="unq.tpi.desapp.model.manager.InscriptionManager"
			not-null="true" />

		<many-to-one name="scoreManager" column="ID_SCORE_MANAGER"
			unique="true" class="unq.tpi.desapp.model.manager.ScoreManager"
			not-null="true" />

		<many-to-one name="commentManager" column="ID_COMMENT_MANAGER"
			unique="true" class="unq.tpi.desapp.model.manager.CommentManager"
			not-null="true" />

	</class>
</hibernate-mapping>
